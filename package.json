{
  "name": "stack-styled",
  "version": "0.0.1",
  "description": "Stacking layouts for React",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me"
  },
  "homepage": "https://github.com/sapegin/stack-styled",
  "repository": "sapegin/stack-styled",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "main": "lib/Stack.js",
  "browser": "esm/Stack.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "npm run test:jest",
    "posttest": "npm run format",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "styleguidist server",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "babel --delete-dir-on-start --ignore '**/*.spec.js' -d esm/ src/",
    "build:cjs": "babel --delete-dir-on-start --ignore '**/*.spec.js' --env-name cjs -d lib/ src/",
    "deploy": "gh-pages -d styleguide-build",
    "docs": "npm run styleguide:build && npm run deploy",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --cache --fix",
    "pretest": "npm run lint",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write '**/*.{js,md}'",
    "precommit": "lint-staged"
  },
  "keywords": [
    "white",
    "space",
    "whitespace",
    "margin",
    "padding",
    "grid",
    "stack",
    "layout",
    "react",
    "component",
    "components",
    "styled-system",
    "styled-components",
    "emotion",
    "design-system",
    "css-in-js"
  ],
  "dependencies": {
    "system-components": "^3.0.0",
    "styled-system": "^3.0.2"
  },
  "peerDependencies": {
    "styled-components": ">=2",
    "emotion": ">=9",
    "react": ">=16",
    "react-emotion": ">=9"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "eslint": "^5.4.0",
    "eslint-config-tamia": "^6.0.2",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.1",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-styleguidist": "^7.3.4",
    "react-testing-library": "^5.0.0",
    "styled-components": "^3.4.5",
    "webpack": "^4.17.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ]
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
